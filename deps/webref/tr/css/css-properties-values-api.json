{
  "spec": {
    "title": "CSS Properties and Values API Level 1",
    "url": "https://www.w3.org/TR/css-properties-values-api-1/"
  },
  "properties": [],
  "atrules": [
    {
      "name": "@property",
      "href": "https://www.w3.org/TR/css-properties-values-api-1/#at-ruledef-property",
      "descriptors": [
        {
          "name": "syntax",
          "href": "https://www.w3.org/TR/css-properties-values-api-1/#descdef-property-syntax",
          "for": "@property",
          "value": "<string>",
          "initial": "n/a (see prose)"
        },
        {
          "name": "inherits",
          "href": "https://www.w3.org/TR/css-properties-values-api-1/#descdef-property-inherits",
          "for": "@property",
          "value": "true | false",
          "initial": "n/a (see prose)"
        },
        {
          "name": "initial-value",
          "href": "https://www.w3.org/TR/css-properties-values-api-1/#descdef-property-initial-value",
          "for": "@property",
          "value": "<declaration-value>?",
          "initial": "the guaranteed-invalid value (but see prose)"
        }
      ],
      "value": "@property <custom-property-name> { <declaration-list> }"
    }
  ],
  "selectors": [],
  "values": []
}
